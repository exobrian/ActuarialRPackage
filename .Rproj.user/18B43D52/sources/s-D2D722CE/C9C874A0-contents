\name{InforceDateRange}
\alias{InforceDateRange}
\title{Date Functions
Add together two numbers}
\usage{
InforceDateRange(EffectiveDate, ExpirationDate, EvaluationDate)
}
\arguments{
\item{EffectiveDate}{Effective Date of policy.}

\item{ExpirationDate}{Expiration Date of policy.}

\item{EvaluationDate}{Date of the evaluation (this will be the date of reference or checking. Typically it'll be current date).}
}
\value{
Boolean indicating whether the policy is inforce at the time of evaluation.
}
\description{
Date Functions
Add together two numbers
}
\examples{
inforceCheckTrue <- InforceDateRange(as.Date("1/1/2020"), as.Date("1/1/2021"), as.Date("7/1/2020))
inforceCheckFalse <- InforceDateRange(as.Date("1/1/2020"), as.Date("1/1/2021"), as.Date("7/1/2021))
}
